# Example .env file for n8n setup
N8N_HOST=your-host-ip-or-domain
N8N_PORT=5678
WEBHOOK_URL=https://your-host-ip-or-domain:5678/
GENERIC_TIMEZONE=Europe/Berlin
N8N_SSL_KEY=/certs/n8n-key.pem
N8N_SSL_CERT=/certs/n8n-cert.pem

# Security additions
REDIS_PASSWORD=your_secure_grafana_password_here
GRAFANA_ADMIN_PASSWORD=your_secure_grafana_password_here

# Optional: for other services
OLLAMA_HOST=0.0.0.0:11434
OLLAMA_NUM_PARALLEL=2  # Parallel requests for GPU